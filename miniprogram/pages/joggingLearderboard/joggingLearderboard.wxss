/* pages/joggingLearderboard/joggingLearderboard.wxss */
.singleUser{
  margin: 10px 10px 10px 10px;
  padding: 5px 5px 5px 5px;
  width: 90%;
  border-radius: 10px;
  background-color: #669933;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.avatar{
  height: 50px;
  width: 50px;
}
.index{
  font-weight: bold;
  margin: 10px 10px 10px 10px;
}
.nickName{
  margin: 10px 10px 10px 10px;
  color: #EEFFEE;
}
.count{
  flex: 1;
  padding-right: 20px;
  text-align: right;
  font-weight: bold;
}